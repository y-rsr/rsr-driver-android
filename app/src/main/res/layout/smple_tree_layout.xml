<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ew_ll_parent_node_container"
    android:layout_width="match_parent"
    android:background="@color/white_color"
    android:layout_height="wrap_content"
    android:gravity="center_vertical"
    android:orientation="horizontal">

    <!--
            <View
                android:visibility="gone"
                android:layout_marginBottom="0dp"
                android:layout_marginTop="0dp"
                android:layout_marginLeft="5dp"
                android:background="@color/black"
                android:layout_width="2dp"
                android:layout_height="match_parent"/>

            <View
                android:visibility="gone"
                android:background="@color/black"
                android:layout_gravity="center"
                android:layout_width="20dp"
                android:layout_height="2dp"/>-->

    <LinearLayout
        android:id="@+id/line_layout"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="match_parent" />

    <LinearLayout
        android:id="@+id/linear_all"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">



        <View
            android:id="@+id/view_line"
            android:layout_marginBottom="0dp"
            android:layout_marginTop="0dp"
            android:background="#565656"
            android:layout_width="1dp"
            android:layout_height="match_parent"/>



        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <View
                android:layout_centerInParent="true"
                android:id="@+id/view_cwntree"
                android:background="#565656"
                android:layout_gravity="center"
                android:layout_width="18dp"
                android:layout_height="1dp"/>

            <View
                android:id="@+id/vert_view"
                android:layout_marginLeft="9.5dp"
                android:layout_below="@+id/view_cwntree"
                android:layout_marginBottom="0dp"
                android:layout_marginTop="0dp"
                android:background="#565656"
                android:layout_width="1dp"
                android:layout_height="match_parent"/>

        </RelativeLayout>



        <LinearLayout
            android:layout_marginBottom="3dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="2dp"
            android:id="@+id/linear_container"
            android:orientation="horizontal"
            android:background="@color/app_color_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="match_parent">
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:textColor="@color/white_color"
                    android:textSize="15sp"
                    android:paddingStart="10dp"
                    android:id="@+id/ew_tv_node_key"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Parent"/>

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_below="@+id/ew_tv_node_key"
                    android:gravity="center|left"
                    android:id="@+id/txt_rank"
                    android:paddingStart="10dp"
                    android:textColor="@color/white_color"
                    android:textSize="13sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Child"/>
            </LinearLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="40dp">
                <Button
                    android:background="@drawable/ex_down"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:visibility="visible"
                    android:textSize="15dp"
                    android:textColor="@android:color/transparent"
                    android:id="@+id/ew_btn_expend"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginRight="20dp"
                    android:padding="0dp"
                    android:text="+"/>
            </RelativeLayout>


        </LinearLayout>


        <TextView
            android:textSize="18sp"
            android:id="@+id/ew_tv_node_size"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_gravity="center"
            android:textColor="@color/change_green"
            android:text="demo///{2}"
            android:visibility="gone"
            tools:visibility="gone"/>

        <TextView
            android:textSize="18sp"
            android:id="@+id/ew_tv_colon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_gravity="center"
            android:textColor="@color/colorPrimary1"
            android:text=":"
            android:visibility="gone"
            tools:visibility="gone"/>

        <TextView
            android:textColor="@color/orange_color"
            android:textSize="18sp"
            android:id="@+id/ew_tv_node_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:visibility="gone"
            tools:visibility="gone"
            android:text="demo///-1"/>

    </LinearLayout>



</LinearLayout>
