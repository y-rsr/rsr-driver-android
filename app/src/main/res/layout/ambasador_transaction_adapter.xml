<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:paddingBottom="20dp"
    android:background="#cdcdcd"
    android:layout_height="wrap_content">

    <android.support.v7.widget.CardView
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="false"
        android:layout_centerHorizontal="true">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="45dp">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:text="Driver Name"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:layout_marginRight="10dp"
                    android:textColor="@color/orange_color"
                    android:text=""
                    android:id="@+id/txt_driver_name"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_height="wrap_content" />

            </RelativeLayout>

            <View
                android:layout_marginTop="5dp"
                android:background="#cdcdcd"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"/>


            <RelativeLayout
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="45dp">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:text="Commission Percent(%)"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:id="@+id/commsio_percent"
                    android:layout_marginRight="10dp"
                    android:textColor="@color/orange_color"
                    android:text=""
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_height="wrap_content" />

            </RelativeLayout>

            <View
                android:layout_marginTop="5dp"
                android:background="#cdcdcd"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"/>

            <RelativeLayout
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="45dp">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:text="Commission"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:id="@+id/txt_commision"
                    android:layout_marginRight="10dp"
                    android:textColor="@color/orange_color"
                    android:text=""
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_height="wrap_content" />

            </RelativeLayout>




            <View
                android:layout_marginTop="5dp"
                android:background="#cdcdcd"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"/>

            <RelativeLayout
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="45dp">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:text="Rank(* at that time of Booking)"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:layout_marginRight="10dp"
                    android:textColor="@color/orange_color"
                    android:text=""
                    android:id="@+id/txt_rank_time"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_height="wrap_content" />

            </RelativeLayout>




            <View
                android:layout_marginTop="5dp"
                android:background="#cdcdcd"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"/>

            <RelativeLayout
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="45dp">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:text="Date"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:layout_marginRight="10dp"
                    android:textColor="@color/orange_color"
                    android:text=""
                    android:id="@+id/txt_date"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_height="wrap_content" />

            </RelativeLayout>



            <View
                android:layout_marginTop="5dp"
                android:background="#cdcdcd"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"/>

            <RelativeLayout
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="45dp">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:text="Driver Level"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:layout_marginRight="10dp"
                    android:textColor="@color/orange_color"
                    android:text=""
                    android:id="@+id/txt_driver_level"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_height="wrap_content" />

            </RelativeLayout>


            <View
                android:layout_marginTop="5dp"
                android:background="#cdcdcd"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"/>

            <RelativeLayout
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="45dp">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:text="Type"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:layout_marginRight="10dp"
                    android:textColor="@color/orange_color"
                    android:text=""
                    android:id="@+id/txt_type"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_height="wrap_content" />

            </RelativeLayout>



            <View
                android:layout_marginTop="5dp"
                android:background="#cdcdcd"
                android:layout_width="match_parent"
                android:layout_height="0.1dp"/>

            <RelativeLayout
                android:layout_marginLeft="10dp"
                android:layout_width="match_parent"
                android:layout_height="45dp">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:text="Payment Status"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:textSize="16sp"
                    android:layout_marginRight="10dp"
                    android:textColor="@color/orange_color"
                    android:text=""
                    android:id="@+id/txt_payment_status"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_height="wrap_content" />

            </RelativeLayout>


        </LinearLayout>

        </android.support.v7.widget.CardView>

</RelativeLayout>
