<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@android:color/transparent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:paddingBottom="10dp"
        android:background="@drawable/white_shape"
        android:layout_margin="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:id="@+id/rel_name"
            android:layout_width="match_parent"
            android:layout_height="40dp">

            <com.ridesharerental.widgets.textviews.Custom_TextView
                android:layout_width="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="@string/docs"
                android:textSize="16sp"
                android:textColor="@color/black"
                android:layout_centerVertical="true"
                android:layout_height="wrap_content" />

        </RelativeLayout>


        <RelativeLayout
            android:layout_marginTop="0dp"
            android:layout_below="@+id/rel_name"
            android:id="@+id/rel_address"
            android:layout_width="match_parent"
            android:layout_height="40dp">

            <com.ridesharerental.widgets.textviews.Custom_TextView
                android:layout_width="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="@string/download_docs"
                android:textSize="16sp"
                android:textColor="@color/black"
                android:layout_centerVertical="true"
                android:layout_height="wrap_content" />

        </RelativeLayout>

        <View
            android:id="@+id/view"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/rel_address"
            android:background="#cdcdcd"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>

        <com.ridesharerental.widgets.ExpandableHeightListView
            android:id="@+id/list_documents"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="10dp"
            android:layout_below="@+id/view"
            android:divider="@null"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </com.ridesharerental.widgets.ExpandableHeightListView>


        <LinearLayout
            android:visibility="gone"
            android:id="@+id/linear_price"
            android:orientation="vertical"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Price"
                    android:textSize="16sp"
                    android:textColor="@color/ligth_gray"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="100"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />

            </LinearLayout>


            <LinearLayout
                android:layout_marginTop="10dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Price"
                    android:textSize="16sp"
                    android:textColor="@color/ligth_gray"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="100"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />

            </LinearLayout>


            <LinearLayout
                android:layout_marginTop="10dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Price"
                    android:textSize="16sp"
                    android:textColor="@color/ligth_gray"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="100"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="10dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Price"
                    android:textSize="16sp"
                    android:textColor="@color/ligth_gray"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="100"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />

            </LinearLayout>


            <LinearLayout
                android:layout_marginTop="10dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Price"
                    android:textSize="16sp"
                    android:textColor="@color/ligth_gray"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="100"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="10dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="Price"
                    android:textSize="16sp"
                    android:textColor="@color/ligth_gray"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />
                <com.ridesharerental.widgets.textviews.Custom_TextView
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:text="100"
                    android:textSize="16sp"
                    android:textColor="@color/black"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content" />

            </LinearLayout>



        </LinearLayout>


        <RelativeLayout
            android:id="@+id/rel_close"
            android:background="@drawable/textview_border"
            android:layout_marginTop="20dp"
            android:gravity="center"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_below="@+id/list_documents"
            android:layout_width="match_parent"
            android:layout_height="45dp">

            <com.ridesharerental.widgets.textviews.Custom_TextView
                android:layout_width="wrap_content"
                android:text="@string/close"
                android:textSize="18sp"
                android:textColor="@color/app_color_1"
                android:layout_height="wrap_content" />

        </RelativeLayout>



    </RelativeLayout>

</RelativeLayout>
