<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="20dp"
    android:orientation="vertical">

    <ScrollView
        android:scrollbars="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">




    <android.support.v7.widget.CardView
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white_color">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="3dp"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:background="@color/colorPrimary">

                        <com.ridesharerental.widgets.textviews.Custom_Textview_Bold
                            android:id="@+id/layout_inflator_reserCarname"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="10dp"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:maxLength="25"
                            android:text=""
                            android:textColor="@color/white_color"
                            android:textSize="18dp" />

                        <com.ridesharerental.widgets.textviews.Custom_Textview_Bold
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="10dp"
                            android:id="@+id/txt_status"
                            android:layout_toRightOf="@+id/layout_inflator_reserCarname"
                            android:gravity="right|center"
                            android:text=""
                            android:visibility="gone"
                            android:textColor="@color/white_color"
                            android:textSize="14dp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:paddingBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <View
                            android:visibility="gone"
                            android:layout_centerVertical="true"
                            android:layout_centerHorizontal="true"
                            android:layout_width="match_parent"
                            android:layout_height="20dp"
                            android:background="@drawable/curver_bg" />

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_marginBottom="00dp"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:weightSum="3">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:layout_weight="1"
                                    android:visibility="gone"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="10dp"
                                    android:src="@drawable/calendar" />

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="120dp"
                                    android:layout_weight="1">

                                    <View
                                        android:layout_centerVertical="true"
                                        android:layout_centerHorizontal="true"
                                        android:layout_width="match_parent"
                                        android:layout_height="20dp"
                                        android:background="@drawable/curver_bg" />

                                    <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                        android:id="@+id/txt_waiting_acceptance"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerHorizontal="true"
                                        android:textAlignment="center"
                                        android:layout_weight="1"
                                        android:text="@string/waiting_acceptance"
                                        android:textColor="@color/app_color_1"
                                        android:textSize="12dp" />

                                    <ImageView
                                        android:id="@+id/img_waiting"
                                        android:layout_centerVertical="true"
                                        android:layout_marginBottom="10dp"
                                        android:layout_width="45dp"
                                        android:layout_height="45dp"
                                        android:layout_centerHorizontal="true"
                                        android:layout_marginTop="5dp"
                                         />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:visibility="gone">



                                    <ImageView
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_centerHorizontal="true"
                                        android:layout_marginTop="5dp"
                                        android:src="@drawable/calendar" />
                                </RelativeLayout>


                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="120dp"
                                    android:layout_weight="1"
                                    android:visibility="visible">

                                    <View
                                        android:layout_centerVertical="true"
                                        android:layout_centerHorizontal="true"
                                        android:layout_width="match_parent"
                                        android:layout_height="20dp"
                                        android:background="@drawable/curver_bg" />

                                    <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                        android:id="@+id/txt_insurance"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerHorizontal="true"
                                        android:textAlignment="center"
                                        android:gravity="center"
                                        android:layout_weight="1"
                                        android:text="@string/insurance_registrton"
                                        android:textColor="@color/app_color_1"
                                        android:textSize="12dp" />



                                    <ImageView

                                        android:layout_centerVertical="true"
                                        android:id="@+id/img_insurance"
                                        android:layout_marginBottom="10dp"
                                        android:layout_width="45dp"
                                        android:layout_height="45dp"
                                        android:layout_centerHorizontal="true"
                                        android:layout_marginTop="5dp"
                                         />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:visibility="gone">



                                    <ImageView
                                        android:layout_width="45dp"
                                        android:layout_height="45dp"

                                        android:layout_centerHorizontal="true"
                                        android:layout_marginTop="5dp"
                                        android:src="@drawable/calendar" />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="120dp"
                                    android:layout_weight="1">


                                    <View
                                        android:layout_centerVertical="true"
                                        android:layout_centerHorizontal="true"
                                        android:layout_width="match_parent"
                                        android:layout_height="20dp"
                                        android:background="@drawable/curver_bg" />


                                    <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                        android:id="@+id/txt_spend"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerHorizontal="true"
                                        android:textAlignment="center"
                                        android:layout_weight="1"
                                        android:text="@string/active_"
                                        android:textColor="@color/app_color_1"
                                        android:textSize="12dp" />



                                    <ImageView
                                        android:id="@+id/img_spend"
                                        android:layout_marginBottom="10dp"
                                        android:layout_centerVertical="true"
                                        android:layout_width="45dp"
                                        android:layout_height="45dp"
                                        android:layout_centerHorizontal="true"
                                        android:layout_marginTop="5dp"
                                         />
                                </RelativeLayout>

                            </LinearLayout>

                        </FrameLayout>


                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">



                        <ImageView
                            android:background="@drawable/placeholdercar"
                            android:id="@+id/img_car"
                            android:layout_width="match_parent"
                            android:layout_height="200dp"
                            android:scaleType="fitXY"
                             />

                        <com.ridesharerental.widgets.CircleImageView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:background="@drawable/icn_profile"
                            android:id="@+id/layout_inflator_reserUserprofile"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="30dp"
                            android:layout_marginTop="170dp"
                             />

                        <com.ridesharerental.widgets.textviews.Custom_TextView
                            android:layout_marginTop="10dp"
                            android:layout_marginLeft="10dp"
                            android:textSize="13sp"
                            android:id="@+id/txt_no_of_days"
                            android:text=""
                            android:gravity="center"
                            android:paddingStart="5dp"
                            android:paddingEnd="5dp"
                            android:textColor="@color/app_color_1"
                            android:background="@drawable/textview_border"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content" />


                        <LinearLayout
                            android:layout_below="@+id/layout_inflator_reserUserprofile"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:orientation="horizontal"
                                android:layout_weight="2"
                                android:layout_height="wrap_content">

                                <LinearLayout
                                    android:visibility="gone"
                                    android:layout_width="match_parent"
                                    android:orientation="vertical"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content">
                                    <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                        android:layout_width="wrap_content"
                                        android:text="@string/price"
                                        android:textSize="14sp"
                                        android:textColor="@color/colorPrimary"
                                        android:layout_height="wrap_content" />
                                    <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                        android:layout_width="wrap_content"
                                        android:visibility="visible"
                                        android:text=""
                                        android:textSize="14sp"
                                        android:id="@+id/txt_total_price"
                                        android:textColor="@color/yellow"
                                        android:layout_height="wrap_content" />
                                </LinearLayout>

                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_marginTop="5dp"
                                android:layout_height="wrap_content">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:orientation="vertical"
                                    android:layout_weight="1"
                                    android:layout_height="wrap_content">
                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_weight="1"
                                        android:orientation="vertical"
                                        android:layout_height="wrap_content">
                                        <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                            android:layout_width="wrap_content"
                                            android:text="@string/renatl_start"
                                            android:textSize="14sp"
                                            android:textColor="@color/colorPrimary"
                                            android:layout_height="wrap_content" />

                                        <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                            android:layout_width="wrap_content"
                                            android:text=""
                                            android:textSize="14sp"
                                            android:layout_marginTop="5dp"
                                            android:id="@+id/txt_check_In"
                                            android:textColor="@color/ligth_gray"
                                            android:layout_height="wrap_content" />
                                    </LinearLayout>
                                    <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                        android:layout_width="wrap_content"
                                        android:text="@string/renatl_end"
                                        android:layout_marginTop="10dp"
                                        android:textSize="14sp"
                                        android:textColor="@color/colorPrimary"
                                        android:layout_height="wrap_content" />
                                    <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                        android:layout_width="wrap_content"
                                        android:text=""
                                        android:textSize="14sp"
                                        android:id="@+id/txt_check_Out"
                                        android:layout_marginTop="5dp"
                                        android:textColor="@color/ligth_gray"
                                        android:layout_height="wrap_content" />


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:orientation="vertical"
                                    android:layout_weight="1"
                                    android:layout_marginRight="7dp"
                                    android:layout_marginLeft="7dp"
                                    android:layout_height="wrap_content">



                                        <com.ridesharerental.widgets.textviews.Custom_Textview_Bold
                                            android:id="@+id/layout_inflator_reservation_extentBTN"
                                            android:layout_width="match_parent"
                                            android:layout_weight="1"
                                            android:text="@string/renatl_extent"
                                            android:background="@drawable/with_radius"
                                            android:textAllCaps="false"
                                            android:textSize="14sp"
                                            android:gravity="center"
                                            android:padding="5dp"
                                            android:textColor="@color/white_color"
                                            android:layout_height="40dp" />

                                        <com.ridesharerental.widgets.textviews.Custom_Textview_Bold
                                            android:id="@+id/layout_inflator_reservation_extend_details"
                                            android:layout_width="match_parent"
                                            android:layout_marginLeft="0dp"
                                            android:text="@string/renatl_extent_details"
                                            android:layout_marginTop="5dp"
                                            android:background="@drawable/with_radius"
                                            android:textAllCaps="false"
                                            android:layout_weight="1"
                                            android:textSize="14sp"
                                            android:gravity="center"
                                            android:padding="5dp"
                                            android:textColor="@color/white_color"
                                            android:layout_height="40dp" />


                                    <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                        android:layout_marginTop="5dp"
                                        android:layout_width="match_parent"
                                        android:text=""
                                        android:textSize="14sp"
                                        android:id="@+id/txt_timer"
                                        android:gravity="center"
                                        android:background="@drawable/gray_border_shape"
                                        android:textColor="@color/white_color"
                                        android:layout_height="40dp" />

                                </LinearLayout>






                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/linear_transaction_invoice"
                                android:weightSum="2"
                                android:orientation="horizontal"
                                android:layout_marginTop="5dp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text="@string/invoice"
                                    android:visibility="visible"
                                    android:padding="5dp"
                                    android:layout_marginRight="5dp"
                                    android:layout_weight="1"
                                    android:textSize="14sp"
                                    android:gravity="center"
                                    android:id="@+id/txt_invoice"
                                    android:background="@drawable/textview_border"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/app_color_1"
                                    android:layout_height="40dp" />

                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text="@string/my_transactions"
                                    android:padding="5dp"
                                    android:layout_marginLeft="5dp"
                                    android:id="@+id/txt_my_Transactions"
                                    android:textSize="14sp"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:background="@drawable/textview_border"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/app_color_1"
                                    android:layout_height="40dp" />




                            </LinearLayout>





                            <View
                                android:layout_width="match_parent"
                                android:layout_marginTop="20dp"
                                android:layout_marginBottom="10dp"
                                android:background="@color/thin_light"
                                android:layout_height="0.1dp"/>

                        </LinearLayout>
                    </RelativeLayout>

                    <RelativeLayout
                        android:visibility="gone"
                        android:id="@+id/rel_details"
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">



                        <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                            android:background="@drawable/textview_border"
                            android:layout_width="250dp"
                            android:text="Detail"
                            android:textSize="18sp"
                            android:textColor="@color/colorPrimary"
                            android:gravity="center"
                            android:layout_height="50dp" />

                    </RelativeLayout>




                    <LinearLayout
                        android:visibility="visible"
                        android:layout_width="match_parent"
                        android:orientation="horizontal"
                        android:weightSum="2"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:layout_weight="1"
                            android:layout_height="wrap_content">

                            <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                android:layout_width="wrap_content"
                                android:text=""
                                android:id="@+id/txt_host_name"
                                android:textSize="14sp"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/colorPrimary"
                                android:layout_height="wrap_content" />
                            <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                android:layout_width="wrap_content"
                                android:text=""
                                android:textSize="14sp"
                                android:id="@+id/txt_address"
                                android:layout_marginTop="5dp"
                                android:textColor="@color/ligth_gray"
                                android:layout_height="wrap_content" />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:layout_weight="1"
                            android:layout_height="wrap_content">
                            <LinearLayout
                                android:weightSum="2"
                                android:visibility="gone"
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text="@string/vin"
                                    android:textSize="14sp"
                                    android:layout_weight="1"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text=""
                                    android:singleLine="true"
                                    android:id="@+id/txt_vin_val"
                                    android:gravity="center|left"
                                    android:layout_weight="1"
                                    android:textSize="14sp"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />

                            </LinearLayout>
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:weightSum="2"
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text="@string/plate"
                                    android:layout_weight="1"
                                    android:textSize="14sp"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text=""
                                    android:id="@+id/txt_plat_no_value"
                                    android:gravity="center|left"
                                    android:textSize="14sp"
                                    android:layout_weight="1"
                                    android:singleLine="true"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />

                            </LinearLayout>
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:weightSum="2"
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text="@string/make"
                                    android:layout_weight="1"
                                    android:textSize="14sp"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text=""
                                    android:id="@+id/txt_make_value"
                                    android:gravity="center|left"
                                    android:layout_weight="1"
                                    android:textSize="14sp"
                                    android:layout_marginTop="5dp"
                                    android:singleLine="true"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />

                            </LinearLayout>
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:weightSum="2"
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text="@string/model"
                                    android:layout_weight="1"
                                    android:textSize="14sp"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text=""
                                    android:id="@+id/txt_model_value"
                                    android:layout_weight="1"
                                    android:textSize="14sp"
                                    android:singleLine="true"
                                    android:gravity="center|left"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />

                            </LinearLayout>
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:weightSum="2"
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text="@string/year"
                                    android:textSize="14sp"
                                    android:layout_weight="1"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text=""
                                    android:id="@+id/txt_year_value"
                                    android:layout_weight="1"
                                    android:textSize="14sp"
                                    android:singleLine="true"
                                    android:gravity="center|left"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:weightSum="2"
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text="@string/vehical_no"
                                    android:textSize="14sp"
                                    android:layout_weight="1"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text=""
                                    android:id="@+id/txt_vehical_no"
                                    android:layout_weight="1"
                                    android:textSize="14sp"
                                    android:singleLine="true"
                                    android:gravity="center|left"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />

                            </LinearLayout>


                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:weightSum="2"
                                android:visibility="gone"
                                android:layout_width="match_parent"
                                android:orientation="horizontal"
                                android:layout_height="wrap_content">
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text="@string/notes"
                                    android:textSize="14sp"
                                    android:layout_weight="1"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/ligth_gray"
                                    android:layout_height="wrap_content" />
                                <com.ridesharerental.widgets.textviews.Custom_Textview_Medium
                                    android:layout_width="match_parent"
                                    android:text=""
                                    android:id="@+id/txt_notes"
                                    android:layout_weight="1"
                                    android:textSize="14sp"
